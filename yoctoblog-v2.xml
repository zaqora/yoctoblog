<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html>
<html b:css='false' b:js='false' b:version='2' expr:dir='data:blog.languageDirection' expr:lang='data:blog.locale' xmlns='http://www.w3.org/1999/xhtml' xmlns:b='http://www.google.com/2005/gml/b' xmlns:data='http://www.google.com/2005/gml/data' xmlns:expr='http://www.google.com/2005/gml/expr'>

<head>
<meta content='width=device-width, initial-scale=1' name='viewport'/>
<b:include data='blog' name='all-head-content'/>
<title><data:view.title.escaped/></title>
<b:skin><![CDATA[

.hidden {
  display: none;
}
]]></b:skin>
<b:template-skin><![CDATA[]]></b:template-skin>
<b:include data='blog' name='google-analytics'/>
</head>

<body expr:class='data:view.type + data:blog.mobileClass'>

<b:if cond='data:view.isMultipleItems'>
<div class='hidden' itemscope='itemscope' itemtype='http://schema.org/Blog'>
<meta expr:content='data:blog.title' itemprop='name'/>
<b:if cond='data:blog.metaDescription'>
<meta expr:content='data:blog.metaDescription' itemprop='description'/>
</b:if>
</div>
</b:if>

<b:section class='blog-header' id='Header' name='Header'>
<b:widget id='Header1' type='Header'/>
</b:section>

<b:section class='blog-menu' id='Menu' name='Menu'>
<b:widget id='PageList1' type='PageList'/>
</b:section>

<b:section class='blog-main' id='Main' name='Main'>
<b:widget id='Blog1' type='Blog'/>
</b:section>

<b:section class='blog-footer' id='Footer' name='Footer'>
<b:widget id='Attribution1' type='Attribution'/>
</b:section>

</body>
</html>