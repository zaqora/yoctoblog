/* Variable */

:root {
  --monospace: SFMono-Regular, Consolas, 'Liberation Mono', Menlo, monospace, monospace;
  --sans-serif: -apple-system, BlinkMacSystemFont, 'Segoe UI', Helvetica, Arial, sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji', 'Segoe UI Symbol';
  --serif: Georgia, serif;

  --teks: #333;
  --teks-sekunder: #666;
  --background: #fff;
  --background-sekunder: #eee;
  --border: #ddd;
  --link: #069;
  --hover: #09c;
}

/* General */

body {
  color: var(--teks);
  font-family: var(--sans-serif);
  font-size: 16px;
  line-height: 1.5;
}

h1, h2, h3, h4, h5, h6 {
  font-family: var(--sans-serif);
  line-height: 1.25;
  margin-top: 0;
  margin-bottom: 0;
}

h1 {
  font-size: 1.5em;
}

h2 {
  font-size: 1.375em;
}

h3 {
  font-size: 1.25em;
}

h4, h5, h6 {
  font-size: 1.125em;
}

a {
  color: var(--link);
  text-decoration: none;
}

a:hover {
  color: var(--hover);
  text-decoration: underline;
}

/* Header */

.Header {
  margin-top: 2.5em;
}

.Header h1 {
  display: inline-block;
  font-size: 1em;
  line-height: 1.5;
}

.Header p {
  display: inline;
  margin-top: 0;
  margin-bottom: 0;
}

/* Menu */

.PageList {
  margin-top: 1em;
}

.PageList ul {
  list-style: none;
  margin: 0;
  padding: 0;
}

.PageList li {
  display: inline-block;
}

.PageList li:not(:last-child) {
  margin-right: 1em;
  margin-bottom: 0.5em;
}

.PageList li.selected {
  font-weight: 700;
}

.PageList li.selected a {
  color: var(--teks);
  text-decoration: underline;
}

.PageList li.selected a:hover {
  color: var(--hover);
  text-decoration: none;
}

/* Content */

.post {
  border-bottom: 1px solid var(--border);
  margin-top: 2.5em;
  padding-bottom: 1.25em;
}

.post-title a {
  color: var(--teks);
}

.post-title a:hover {
  color: var(--hover);
}

.post-header {
  margin-top: 0.5em;
}

.post-header a {
  color: var(--teks-sekunder);
}

.post-body {
  margin-top: 1.25em;
}

.post-body a {
  text-decoration: underline;
}

.post-body a:hover {
  text-decoration: none;
}

/* Pager */

.blog-pager {
  margin-top: 2.5em;
}

/* Layout */

.widget {
  margin-right: auto;
